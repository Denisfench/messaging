{
    "required": ["datetime", "zipcode", "city", "state", "country", "type", "priority", "sender"],
    "properties": {
        "datetime": {
            "required": true,
            "type": "datetime",
            "description": "When did event occur?",
            "example": "2020-03-14 12:45:32"
        },
        "zipcode": {
            "required": true,
            "type": "string",
            "example": "10001"
        },
        "city": {
            "required": true,
            "type": "string",
            "example": "New York City"
        },
        "state": {
            "required": true,
            "type": "string",
            "example": "New York"
        },
        "country": {
            "required": true,
            "type": "string",
            "example": "USA"
        },
        "type": {
            "required": true,
            "type": "string",
            "description": "What is the type of emergecy?",
            "example": "Fire"
        },
        "description": {
            "required": false,
            "type": "string",
            "description": "What is the emergency?",
            "example": "Smoke in the building"
        },
        "priority": {
            "required": true,
            "type": "string",
            "values": ["High", "Medium", "Low"],
            "description": "How severe is the emergency?",
            "example": "Medium"
        },
        "sender": {
            "required": true,
            "type": "string",
            "description": "Who is sending the message?",
            "example": "Socnet API Server"
        }
    }
}
